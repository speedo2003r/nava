{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "apility/laravel-fcm": "^1.4",
        "beyondcode/laravel-websockets": "^1.13",
        "doctrine/dbal": "^3.3",
        "dyrynda/laravel-cascade-soft-deletes": "^4.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "graham-campbell/throttle": "^8.2",
        "guzzlehttp/guzzle": "^7.4",
        "khrdev/awt": "dev-master",
        "laravel/framework": "^8.42",
        "laravel/telescope": "^4.8",
        "laravel/tinker": "^2.5",
        "lcobucci/jwt": "^4.1",
        "niklasravnsborg/laravel-pdf": "^4.1",
        "prettus/l5-repository": "^2.7",
        "pusher/pusher-php-server": "^7.0",
        "spatie/laravel-translatable": "^4.5",
        "stichoza/google-translate-php": "^4.1",
        "tymon/jwt-auth": "^1.0.0",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.12",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/lang": "^10.7",
        "laravel-lang/publisher": "^12.2",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helper.php",
            "app/Helpers/permissions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
